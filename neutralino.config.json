{
  "applicationId": "js.neutralino.whitedove",
  "version": "1.0.0",
  "defaultMode": "window",
  "port": 0,
  "documentRoot": "/distribution/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "tokenSecurity": "none",
  "logging": {
    "enabled": false,
    "writeToLogFile": true
  },
  "cli": {
    "binaryName": "WhiteDove",
    "resourcesPath": "/distribution/",
    "extensionsPath": "/extensions/",
    "clientLibrary": "/distribution/js/neutralino.js",
    "binaryVersion": "nightly",
    "clientVersion": "nightly",
    "frontendLibrary": {
      "patchFile": "/distribution/index.html",
      "devUrl": "http://localhost:54321"
    }
  },
  "nativeAllowList": [
    "app.*",
    "os.*",
    "debug.log",
    "filesystem.*",
    "computer.*"
  ],
  "modes": {
    "window": {
      "title": "WhiteDove",
      "width": 800,
      "height": 500,
      "minWidth": 400,
      "minHeight": 200,
      "fullScreen": false,
      "alwaysOnTop": false,
      "icon": "/distribution/icons/appIcon.png",
      "enableInspector": true,
      "borderless": false,
      "maximize": false,
      "hidden": false,
      "resizable": true,
      "exitProcessOnClose": false
    }
  }
}
